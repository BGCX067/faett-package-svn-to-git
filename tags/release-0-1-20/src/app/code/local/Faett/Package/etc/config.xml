<config>
    <modules>
        <Faett_Package>
            <version>${version}</version>
        </Faett_Package>
    </modules>
    <global>
        <fieldsets>
            <package_sales_copy_order>
                <entity_id><to_package>order_id</to_package></entity_id>
                <increment_id><to_package>order_increment_id</to_package></increment_id>
                <created_at><to_package>*</to_package></created_at>
                <updated_at><to_package>*</to_package></updated_at>
                <customer_id><to_package>*</to_package></customer_id>
            </package_sales_copy_order>
            <package_sales_copy_order_item>
                <item_id><to_package>order_item_id</to_package></item_id>
                <name><to_package>product_name</to_package></name>
                <product_id><to_package>product_id</to_package></product_id>
            </package_sales_copy_order_item>
            <package_sales_copy_link>
                <link_id><to_purchased>*</to_purchased></link_id>
                <title><to_purchased>link_title</to_purchased></title>
                <link_file><to_purchased>*</to_purchased></link_file>
            </package_sales_copy_link>
        </fieldsets>
        <sales>
            <quote>
                <item>
                    <product_attributes>
                        <links_purchased_separately/>
                        <links_title/>
                    </product_attributes>
                </item>
            </quote>
        </sales>
        <models>
            <package>
                <class>Faett_Package_Model</class>
                <resourceModel>package_mysql4</resourceModel>
            </package>
            <package_mysql4>
                <class>Faett_Package_Model_Mysql4</class>
                <entities>
                    <link>
                        <table>faett_package_link</table>
                    </link>
                    <link_title>
                        <table>faett_package_link_title</table>
                    </link_title>
                    <link_price>
                        <table>faett_package_link_price</table>
                    </link_price>
                    <sample>
                        <table>faett_package_sample</table>
                    </sample>
                    <sample_title>
                        <table>faett_package_sample_title</table>
                    </sample_title>
                    <link_purchased>
                        <table>faett_package_link_purchased</table>
                    </link_purchased>
                    <package_maintainer>
                        <table>faett_package_maintainer</table>
                    </package_maintainer>
                    <package_subscription_type>
                        <table>faett_package_subscription_type</table>
                    </package_subscription_type>
                </entities>
            </package_mysql4>
        </models>
        <blocks>
            <package>
                <class>Faett_Package_Block</class>
            </package>
        </blocks>
        <resources>
            <package_setup>
                <setup>
                    <module>Faett_Package</module>
                    <class>Faett_Package_Model_Resource_Eav_Mysql4_Setup</class>
                </setup>
                <connection>
                    <use>core_setup</use>
                </connection>
            </package_setup>
            <package_write>
                <connection>
                    <use>core_write</use>
                </connection>
            </package_write>
            <package_read>
                <connection>
                    <use>core_read</use>
                </connection>
            </package_read>
        </resources>
        <catalog>
            <product>
                <type>
                    <package translate="label" module="package">
                        <label>Package/Extension Product</label>
                        <model>package/product_type</model>
                        <is_qty>1</is_qty>
                        <price_model>package/product_price</price_model>
                        <index_data_retreiver>package/catalogIndex_data_package</index_data_retreiver>
                        <composite>0</composite>
                    </package>
                    <bundle>
                        <allowed_selection_types>
                            <package />
                        </allowed_selection_types>
                    </bundle>
                    <configurable>
                        <allow_product_types>
                            <package />
                        </allow_product_types>
                    </configurable>
                    <grouped>
                        <allow_product_types>
                            <package />
                        </allow_product_types>
                    </grouped>
                </type>
            </product>
        </catalog>
        <pdf>
